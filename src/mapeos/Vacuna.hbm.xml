<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Vacuna" table="vacuna">

		<id column="idVacuna" name="idVacuna">
			<generator class="identity" />
		</id>

		<property column="codigo" name="codigo" type="long" />
		<property column="nombre" name="laboratorio" type="string" />
		<property column="fechaElaboracion" name="fechaElaboracion" type="LocalDate" />

	</class>
</hibernate-mapping>
